let all = {!Alice, !Bob, !Charlie} in

let w = wire all:100 in

let o @ sec(all) =
  let out = makesh w[!Alice] in
  out
in

let m @ sec(all) =
  let t   = w[!Alice] in
  let v   = combsh o in
  let inc = v + 1 in
  let out = makesh inc in  
  out
in

let f @ sec(all) =
  let t   = w[!Alice] in
  let out = combsh m in
  out
in

f
